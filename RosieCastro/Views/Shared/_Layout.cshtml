    <div class="body">

        <!-- Main site header -->
        <header class="site-header text-center">
            <h1 class="animated delay1 fadeIn"><img src="img/title.png" alt="ROSIE CASTRO"></h1>

            <!-- Main navigation -->
            <nav class="navbar animated delay2 fadeInDown">
                <div class="navbar-inner">
                    <div class="container">
                        <div data-toggle="collapse" data-target=".nav-collapse">
                            <span class="pull-left nav-label">Menu</span>
                            <a class="btn btn-navbar">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </a>
                        </div>
                        <div class="nav-collapse collapse">
